#include <stdio.h>


int soma (int a, int b){
    return a+b;
}
int subtracao (int A, int B){
    return A-B;
}
int maior (int x, int y){
        return (x>y)?x:y;
}

int main (){
    int a=10; 
    int b=1;
    printf("Soma: %d\n", soma(a,b));
    printf("Subtracao: %d\n", subtracao(a,b));
    printf("Maior: %d\n", maior(a,b));
    
    
    
}
